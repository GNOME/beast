AddDefaultCharset utf-8

# --- enable mod_rewrite ---
RewriteEngine On

# --- Wiki rewriting ---
# rewrite beast.gtk.org/wiki:BeastFeatures?action=edit to beast.gtk.org/wiki.phtml?action=edit&id=BeastFeatures
RewriteRule wiki:(.*)$		wiki.phtml?id=$1  [L,QSA]	# append query parameters  

# --- PHP configuration ---
# config values are setup before PHP starts
# allow uploads for up to ~8MB
php_value  upload_max_filesize 8388608
php_value  post_max_size       8912896
php_value  memory_limit        9437184


# --- Redirects ---
# 301 - Moved Permanently
# 302 - Found (resides temporarily under different URI)

# Miscellaneous redirects
Redirect 301 /wiki				http://beast.gtk.org/wiki:BeastBse
Redirect 301 /wiki/				http://beast.gtk.org/wiki:BeastBse
Redirect 301 /wiki.php				http://beast.gtk.org/wiki:BeastBse
#Redirect 301 /wiki.phtml			http://beast.gtk.org/wiki:
Redirect 301 /wiki:RamboKidBlog			http://blogs.gnome.org/timj
Redirect 302 /xmlns:xdef			http://beast.gtk.org/documentation.html
Redirect 302 /screenshots.html			http://beast.gtk.org/screenshots.phtml
Redirect 302 /mailinglist.html			http://beast.gtk.org/contact.html
Redirect 302 /bugs.html				http://beast.gtk.org/contact.html
# in the above, the following list of entry points should be made sure to exist:
# download.html, screenshots.html, mailinglist.html, bugs.html, faq.html, about.html

